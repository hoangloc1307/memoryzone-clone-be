generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

enum Role {
  USER
  ADMIN
}

model Account {
  id       String @id @default(uuid())
  email    String @unique
  password String
  role     Role   @default(USER)
}
